{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.5",
        "bensampo/laravel-enum": "^3.1.0",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.3",
        "guzzlehttp/guzzle": "^7.2.0",
        "laravel/framework": "^8.2.0",
        "laravel/sanctum": "^2.7.0",
        "laravel/tinker": "^2.4.2",
        "laravel/ui": "^3.0.0",
        "pusher/pusher-php-server": "^4.1.4",
        "spatie/data-transfer-object": "^2.5.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8.1",
        "facade/ignition": "^2.4.1",
        "fzaninotto/faker": "^1.9.1",
        "laravel/telescope": "^4.2.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0.2",
        "phpunit/phpunit": "^9.4.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
